{
    "name": "babdev/websocket-server",
    "type": "library",
    "description": "PHP library to create a WebSocket server",
    "keywords": [
        "WAMP",
        "websocket",
        "websocket server"
    ],
    "license": "MIT",
    "require": {
        "php": "8.2.* || 8.3.*",
        "guzzlehttp/psr7": "^2.4",
        "psr/http-message": "^1.0 || ^2.0",
        "ratchet/rfc6455": "^0.3.1",
        "react/event-loop": "^1.3",
        "react/socket": "^1.11",
        "symfony/http-foundation": "^6.3 || ^7.0",
        "symfony/routing": "^6.3 || ^7.0"
    },
    "require-dev": {
        "ext-session": "*",
        "ext-sockets": "*",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "1.10.41",
        "phpstan/phpstan-phpunit": "1.3.15",
        "phpunit/phpunit": "10.4.2",
        "psr/container": "^1.0 || ^2.0",
        "psr/event-dispatcher": "^1.0",
        "rector/rector": "0.18.7"
    },
    "conflict": {
        "psr/container": ">=3.0",
        "psr/event-dispatcher": ">=2.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\WebSocket\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\WebSocket\\Server\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev"
}
