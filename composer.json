{
    "name": "babdev/websocket-server",
    "type": "library",
    "description": "PHP library to create a WebSocket server",
    "keywords": [
        "websocket"
    ],
    "license": "MIT",
    "require": {
        "php": "8.1.*",
        "guzzlehttp/psr7": "^2.2",
        "psr/http-message": "^1.0",
        "ratchet/rfc6455": "^0.3.1",
        "react/event-loop": "^1.3",
        "react/socket": "^1.11"
    },
    "require-dev": {
        "ext-sockets": "*",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "1.7.5",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\WebSocket\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\WebSocket\\Server\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev"
}
